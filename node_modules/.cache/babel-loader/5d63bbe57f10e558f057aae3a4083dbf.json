lass=\"form-control\" id=\"nro_habitaciones\">\r\n                </div>\r\n            </div>\r\n            <div class=\"row mt-3\" >\r\n                <div class=\"col\">\r\n                    <label for=\"ciudad\" class=\"form-label\">Ciudad</label>\r\n                    <select class=\"form-select\" id=\"ciudad\" v-model=\"hotel.city_id\" >\r\n                        <option v-for=\"ciudad in cities\" :value=\"ciudad.id\" :key=\"ciudad.id\">{{ ciudad.name }}</option>\r\n                    </select>\r\n                </div> \r\n                <div class=\"col\">\r\n                    \r\n                </div>\r\n            </div>\r\n            <div class=\"row mt-3\">\r\n                <div class=\"col\">\r\n                    <button @click=\"sendHotel\" type=\"submit\" class=\"btn btn-primary \">Enviar</button>\r\n                </div> \r\n                <div class=\"col\">\r\n                    \r\n                </div>\r\n            </div>\r\n    \r\n            <div v-if=\"info\" class=\"alert alert-primary mt-3\" role=\"alert\">\r\n                {{info}}\r\n            </div>\r\n    \r\n    \r\n            \r\n        \r\n        \r\n        \r\n        </div>\r\n    </div>\r\n    </template>\r\n    \r\n    \r\n    <script>\r\n    \r\n    import axios from 'axios'\r\n    \r\n    export default {\r\n        beforeMount() {\r\n            axios\r\n                .get('http://ec2-44-201-108-206.compute-1.amazonaws.com/decameron/api/cities')\r\n                .then( response => (this.cities = response.data) )\r\n        },\r\n        data() {\r\n            return {\r\n                errores: {\r\n                    nit: null\r\n                },\r\n                info: null,\r\n                cities: [],\r\n                hotel: {\r\n                    name: null,\r\n                    nit: null,\r\n                    address: null,\r\n                    num_rooms: null,\r\n                    city_id: null\r\n                }\r\n            }\r\n        },\r\n        methods: {\r\n            viewHotel() {\r\n                axios({\r\n                    method: 'post',\r\n                    url: 'http://ec2-44-201-108-206.compute-1.amazonaws.com/decameron/api/hotels/'+ this.$route.params.id,\r\n                    data: this.hotel,\r\n                    responseType: 'json',\r\n                }) \r\n                .then(response => {  \r\n                    this.info = response.data.message\r\n                    \r\n                    this.hotel.name = null\r\n                    this.hotel.nit = null\r\n                    this.hotel.address = null\r\n                    this.hotel.num_rooms = null\r\n                    this.hotel.city_id = null\r\n                    \r\n                })\r\n                .catch(error => {\r\n                    this.errores = error.response.data.errors\r\n                    \r\n                })            \r\n            }\r\n        },\r\n    }\r\n    \r\n    </script>"]},"metadata":{},"sourceType":"module"}